<?xml version="1.0" encoding="utf-8" ?>
<pge:LagoVistaContentPage
        xmlns:pge="clr-namespace:LagoVista.XPlat.Core;assembly=LagoVista.XPlat.Core"
        xmlns="http://xamarin.com/schemas/2014/forms"
        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
        xmlns:trs="clr-namespace:LagoVista.Simulator;assembly=LagoVista.Simulator"
        x:Class="LagoVista.Simulator.Views.Messages.MessageHeaderView"
        LeftMenu="Back"
        RightMenu="Save"
        Title="{trs:Translate MessageHeader_Title}">

    <pge:LagoVistaContentPage.MainContent>
        <pge:FormViewer Form="{Binding FormAdapter}" />
    </pge:LagoVistaContentPage.MainContent>
</pge:LagoVistaContentPage>